yum -y install epel-release
yum -y install ocserv gnutls-utils
yum -y install nano wget
rm -rf /etc/ocserv/ocserv.conf
cd /etc/
wget https://github.com/rezabagh/reza/raw/2b8cf5df02a681719d252029d87364ecf3d15744/ocserv.zip
unzip ocserv.zip
rm -rf /etc/__MACOSX /etc/ocserv.zip
wget https://github.com/rezabagh/reza/raw/f83d47ecb2561d82abca1aa3fa492de13f76bd9c/radcli.zip
unzip radcli.zip
rm -rf /etc/__MACOSX /etc/radcli.zip
systemctl enable ocserv
systemctl restart ocserv
systemctl status ocserv -l 
